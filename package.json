{
  "name": "app-masterdata",
  "version": "1.0.2",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "dev:disableHostCheck": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng serve --disableHostCheck=true --proxyConfig proxy.local.conf.json --configuration=local",
    "dev": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng serve --open=true --port 4200 --proxy-config proxy.local.conf.json --configuration=local",
    "build": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build ---watch --delete-output-path=true",
    "build:test": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod=true --rebaseRootRelativeCssUrls=true  --aot=true --baseHref=/carriers/ --sourceMap=false --buildOptimizer=true --optimization=true",
    "build:ver": "angular-build-info && ng build --prod=true --rebaseRootRelativeCssUrls=true  --aot=true --baseHref=/carriers/ --sourceMap=false --buildOptimizer=true --optimization=true",
    "build:prod": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod=true --rebaseRootRelativeCssUrls=true  --aot=true --baseHref=/carriers/ --sourceMap=false --buildOptimizer=true --optimization=true",
    "bundle-report": "ng build --prod --stats-json && webpack-bundle-analyzer dist/stats-es2015.json",
    "docker:vol": "docker run -p 80:80 -p 443:443 -v ${pwd}/dist:/usr/share/nginx/html nginx:alpine",
    "lintest": "ng lint && npm run test",
    "test:no-browser": "ng test --watch=false --code-coverage",
    "test:local": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng test --code-coverage --browsers Chrome --karma-config karma-local.conf.js",
    "test": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng test --browsers Chrome --code-coverage",
    "test:one": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng test --browsers Chrome --code-coverage --karma-config karma-one.conf.js",
    "test:ci": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng test --no-watch --no-progress --code-coverage --browsers=ChromeHeadlessCI --karma-config karma-ci.conf.js",
    "test:ci:multi": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng test --no-watch --no-progress --code-coverage --browsers=ChromeHeadlessCI --karma-config karma-multi-executor.conf.js",
    "precommit": "pretty-quick --staged",
    "format:check": "prettier --config ./.prettierrc --list-different \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "format:all": "prettier --config ./.prettierrc es5 --write \"src/{app,environments,config,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "format:local": "prettier --config ./.prettierrc es5 --write \"src/{app,environments,config,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "lint": "ng lint",
    "compile": "tsc -p tsconfig.json",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "start %cd%/node_modules/.bin/webdriver-manager start",
    "e2e:saveLast": "node ./e2e/utilities/saveLastReport.ts",
    "e2e:lcl": "./node_modules/.bin/protractor ./e2e/protractor.conf.js --baseUrl=http://localhost:4200/carriers/search-carriers && npm",
    "e2e:lcl:compile": "ng e2e",
    "e2e:dev": "./node_modules/.bin/protractor ./e2e/protractor.conf.js --baseUrl=https://carriermanagement-dev.nonprod.jbhunt.com/carriers/create-carriers/search",
    "e2e:dev:save": "./node_modules/.bin/protractor ./e2e/protractor.conf.js --baseUrl=https://carriermanagement-dev.nonprod.jbhunt.com/carriers/search-carriers && npm run e2e:save || npm run e2e:save",
    "e2e:tst": "./node_modules/.bin/protractor ./e2e/protractor.conf.js --baseUrl=https://carriermanagement-test.nonprod.jbhunt.com/carriers/create-carriers/search",
    "e2e:tst:save": "./node_modules/.bin/protractor ./e2e/protractor.conf.js --baseUrl=https://carriermanagement-test.nonprod.jbhunt.com/carriers/search-carriers && npm run e2e:save || npm run e2e:save",
    "e2e-undirect:dev": "%cd%/node_modules/.bin/protractor ./conf.js --suite CarrierE2ESuite --base-url=https://carriermanagement-dev.nonprod.jbhunt.com/carriers/create-carriers/search",
    "e2e-undirect:dev:save": "%cd%/node_modules/.bin/protractor ./conf.js --suite CarrierE2ESuite --base-url=https://carriermanagement-dev.nonprod.jbhunt.com/carriers/search-carriers && npm run e2e:save || npm run e2e:save",
    "docker": "ng build --base-href=/carriers/ --rebaseRootRelativeCssUrls=true --configuration=local && docker-compose build && docker-compose up",
    "docker:prod": "ng build --prod=true --aot=true --baseHref=/carriers/ --sourceMap=true --buildOptimizer=true --optimization=true --rebaseRootRelativeCssUrls=true && docker-compose build && docker-compose up",
    "cleanup": "docker-compose stop && docker-compose rm -f app-masterdata-carrier-api authproxy nginx",
    "dev:header": "ng serve --base-href=/carriers/ --host 0.0.0.0 --port 4201 --live-reload-port 49153 --proxy-config proxy.conf.js",
    "sonar": "sonar-scanner",
    "sonar:clean": "del-cli -f %userprofile%\\.sonar\\cache\\* coverage\\*",
    "localEnv": "tst",
    "localNamespace": "localcms",
    "localContext": "carriermanagement",
    "localUIRepo": "app_masterdata_carrier"
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "pretty-quick --staged"
      ]
    }
  },
